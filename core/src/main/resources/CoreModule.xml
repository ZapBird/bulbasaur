<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd"
	   default-autowire="byName">

	<context:annotation-config/>
	<!-- <context:component-scan base-package="com.alibaba.pokemon.bulbasaur.core"/> -->
	<bean id="place" class="com.alibaba.pokemon.bulbasaur.core.Place"/>
	<bean id="parser" class="com.alibaba.pokemon.bulbasaur.core.Parser"/>
	<bean id="machineFactory" class="com.alibaba.pokemon.bulbasaur.core.MachineFactory"/>

</beans>
